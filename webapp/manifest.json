{
	"_version": "1.12.0",
	"sap.app": {
		"id": "focus.customersupportsystem.CustomerSupportSystem",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"ach": "CA-UI5-FST",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/EPM_REF_APPS_PROD_MAN_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "focus.customersupportsystem.CustomerSupportSystem.view.ToolPage",
			"type": "XML",
			"async": true,
			"id": "toolPage"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "focus.customersupportsystem.CustomerSupportSystem.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"defaultBindingMode": "TwoWay"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"viewType": "XML",
				"viewPath": "focus.customersupportsystem.CustomerSupportSystem.view",
				"transition": "slide",
				"controlId": "fcl",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "addticket",
					"name": "addticket",
					"layout": "EndColumnFullScreen",
					"target": [
						"addticket"
					]
				},
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "detailDetail/{product}/{supplier}/{layout}",
					"name": "detailDetail",
					"target": [
						"master",
						"detail",
						"detailDetail"
					]
				}
			],
			"targets": {
				"master": {
					"viewName": "TicketsList",
					"controlAggregation": "beginColumnPages"
				},
				"addticket": {
					"viewName": "AddTicket",
					"controlAggregation": "endColumnPages"
				},
				"detail": {
					"viewName": "TicketDetail",
					"controlAggregation": "midColumnPages"
				},
				"detailDetail": {
					"viewName": "CompanyDetail",
					"controlAggregation": "endColumnPages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}